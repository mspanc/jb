@keyframes fadeIn
  0%
    opacity 0
  100%
    opacity 1

@keyframes fadeOut
  0%
    opacity 1
  100%
    opacity 0

@keyframes fade
  0%
    opacity 1
  50%
    opacity 0
  100%
    opacity 1

@keyframes rotate
  from
    transform-origin: center;
    transform: rotate(0deg);
  to
    transform-origin: center;
    transform: rotate(360deg);
